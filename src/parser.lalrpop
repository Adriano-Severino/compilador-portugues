// === IDENTIFICADORES ===
IdentificadorCompleto: String = {
    <primeiro:TIdentificador> <resto:("." <TIdentificador>)*> => {
        let mut resultado = primeiro;
        for parte in resto {
            resultado.push('.');
            resultado.push_str(&parte);
        }
        resultado
    }
};

