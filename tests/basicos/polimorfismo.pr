classe Animal {
    metodo emitirSom() {
        escreva "Som genérico de animal"
    }
}

classe Cachorro herda Animal {
    metodo emitirSom() {
        escreva "Au au!"
    }
}

classe Gato herda Animal {
    metodo emitirSom() {
        escreva "Miau!"
    }
}

função principal() {
    animal1 = Animal()
    animal2 = Cachorro()
    animal3 = Gato()

    animal1.emitirSom()
    animal2.emitirSom()
    animal3.emitirSom()
}
